    "unix-package": "rm -r dist && ng-packagr -p projects/ngx-extended-pdf-viewer/ng-package.json && cp -R projects/ngx-extended-pdf-viewer/src/assets dist/ngx-extended-pdf-viewer/assets",
    "win-package": "rmdir dist /S && ng-packagr -p projects/ngx-extended-pdf-viewer/ng-package.json && xcopy /S/E/-Y projects\\ngx-extended-pdf-viewer\\src\\assets dist\\ngx-extended-pdf-viewer\\assets\\",
    "release": "npm run package | cd dist/ngx-extended-pdf-viewer | npm publish",
    "updatedemo": "npm run package | rm -r ../ExploringAngular/embedding-pdf/node_modules/ngx-extended-pdf-viewer | cp -r dist/ngx-extended-pdf-viewer  ../ExploringAngular/embedding-pdf/node_modules"
  },
